# Relative position and variance with respect to the vehicle.
# Vehicle is (0,0). X-Y coordinate system (in m) is NED oriented,
# with X-axis pointing to the right and Y-axis forward.

std_msgs/Header header

float32 x
float32 y
float32 x_variance
float32 y_variance

float32 range
float32 bearing
float32 range_variance
float32 bearing_variance

float32 altitude
float32 altitude_variance

float32 heading
